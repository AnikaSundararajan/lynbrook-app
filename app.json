{
  "expo": {
    "name": "Monta Vista High School",
    "slug": "mvhs-app",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mvhs",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4B2D9B"
    },
    "primaryColor": "#4B2D9B",
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.fuhsd.mvhs.app",
      "buildNumber": "2.2.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow camera access to scan barcodes and get spirit points for events."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4B2D9B"
      },
      "package": "org.fuhsd.mvhs.app",
      "versionCode": 26,
      "permissions": ["CAMERA"],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "lynbrook-asb",
            "project": "mvhs"
          }
        }
      ]
    }
  }
}
